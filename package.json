{
  "author": "Lucas Hrabovsky <lucas@ex.fm>",
  "name": "mambo",
  "description": "The best document mapper for DynamoDB.",
  "version": "0.2.5",
  "homepage": "http://github.com/imlucas/node-mambo",
  "repository": {
    "type": "git",
    "url": "git://github.com/imlucas/node-mambo.git"
  },
  "scripts": {
    "test": "MAMBO_BACKEND=magneto ./node_modules/.bin/mocha",
    "docs": "mott build",
    "pushdocs": "mott deploy"
  },
  "dependencies": {
    "plog": "0.2.1",
    "aws-sdk": "1.2.0",
    "async": "0.2.9",
    "underscore": "1.4.4",
    "magneto": "0.3.1"
  },
  "devDependencies": {
    "mocha": "1.10.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  },
  "keywords": [
    "amazon",
    "aws",
    "dynamodb"
  ],
  "mott": {
    "recipe": {
      "build": "pages",
      "deploy": ["build", "deploy to github"],
      "metadata": {
        "pages": {
          "./docs/index.jade": "./index.html"
        }
      }
    }
  }
}
